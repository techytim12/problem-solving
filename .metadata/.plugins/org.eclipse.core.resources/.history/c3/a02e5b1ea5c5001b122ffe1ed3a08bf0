
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {

	int i,c;
	char ch[100][100];
	scanf("%d",&c);

	for (i=0; i<c; i++){
		scanf("%s",ch[i]);
	}

	for (i=0; i<c; i++){
		printf("%s ",ch[i]);
	}

	int y=0,x=0;
	for (i=0; i<c; i++){
		if (strcmp(ch[i],"RIGHT") == 0 && y<c){
			printf("inside right");
			}
		else if (strcmp(ch[i],"LEFT") == 0 && y>0){
			printf("inside left");
		}
		else if (strcmp(ch[i],"UP") == 0 && x>0){
			printf("inside up");
		}
		else if (strcmp(ch[i],"DOWN") == 0 && x<c){
			printf("inside down");
		}
	}




	return EXIT_SUCCESS;
}
