#include <stdio.h>
#include <string.h>

int main(){

	int a[26]={0},i,index;
	char str[100];

	scanf("%s",str);
	int l = strlen(str);

	for (i=0; i<l; i++)
		a[str[i]-97]++;

	printf("\n");
	for (i=0; i<26; i++)
		printf("%d ",a[i]);

	/*printf("\n");
	printf("\n");
	printf("%d",a[str[2]-97]);*/

	for (i=0; i<l; i++)
	{
		if (a[str[i]-97] == 1)
		{
			index = i;
			break;
		}
	}

	printf("%c",str[index]);

}
