#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {

	int i,j,k=0,n,c;

	scanf("%d%d",&n,&c);
	int arr[n][n];
	char ch[][100];

	for (i=0; i<n; i++)
	{
		for (j=0; j<n; j++)
		{
			arr[i][j] = k;
			k++;
		}
	}

	for (i=0; i<n; i++)
	{
		for (j=0; j<n; j++)
		{
			printf("%d ",arr[i][j]);
		}
		printf("\n");
	}



	for (i=0; i<c; i++)
	{
		scanf("%s",ch[i]);
	}

	for (i=0; i<c; i++)
	{
		printf("%s",ch[i]);
	}


	return EXIT_SUCCESS;
}
