#include <stdio.h>

int main(){

	int n,ar[n];
	scanf("%d",&n);

	int i;

	for (i=0; i<n; i++)
		scanf("%d",&ar[i]);

	int a[100]={0},count=0;

	for (i=0; i<n; i++)
		a[ar[i]]++;

	for (i=1; i<=100; i++){
			if (a[i] != 0)
				a[i] = a[i] / 2;
		}

	for (i=1; i<=100; i++){
			if (a[i] != 0)
				count = count + a[i];
		}

	return 0;
}
